#include <bits/stdc++.h>
using namespace std;

int main() {
    int x, y;
    cin >> x;
    int a[x];

    for (int i = 0; i < x; ++i) {
        cin >> a[i];
    }

    cin >> y;

    for (int i = 0; i < x; ++i) {
        if (a[i] < y) {
            cout << i + 1 << " " << a[i] << endl;
        }
    }
}
